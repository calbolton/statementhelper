<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:StatementHelper.ViewModels"
                    xmlns:vw="clr-namespace:StatementHelper.Views">
    <DataTemplate DataType="{x:Type vm:MainWindowViewModel}">
        <vw:MainWindowView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BudgetViewModel}">
        <vw:BudgetView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BudgetSearchViewModel}">
        <vw:BudgetSearchView />
    </DataTemplate>
</ResourceDictionary>